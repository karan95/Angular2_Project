    "@angular/router": "~3.4.0",
    "core-js": "^2.4.1",
    "@angular/platform-server": "^2.4.2",
    "@angular/upgrade": "~2.4.2",
    "http-server": "^0.9.0",
    "rxjs": "5.0.1",
    "zone.js": "^0.7.4"
  },
